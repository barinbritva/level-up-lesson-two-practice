{
  "name": "green-fair",
  "version": "1.0.0",
  "scripts": {
    "clean-build": "rm -rf ./public/js",
    "tsc-build": "npm run clean-build && tsc --outDir ./public/js",
    "tsc-build:dev": "npm run clean-build && tsc --outDir ./public/js --watch",
    "wp-build": "npm run clean-build && NODE_ENV=production webpack",
    "wp-build:dev": "npm run clean-build && NODE_ENV=development webpack",
    "build": "NODE_ENV=production gulp",
    "build:dev": "NODE_ENV=development gulp"
  },
  "license": "UNLICENSED",
  "devDependencies": {
    "browserify": "^17.0.0",
    "del": "^6.0.0",
    "gulp": "^4.0.2",
    "gulp-changed": "^4.0.2",
    "gulp-imagemin": "^7.1.0",
    "gulp-sass": "^4.1.0",
    "node-sass": "^5.0.0",
    "ts-loader": "^8.0.11",
    "tsify": "^5.0.2",
    "typescript": "^4.1.2",
    "vinyl-source-stream": "^2.0.0",
    "watchify": "^3.11.1",
    "webpack": "^5.9.0",
    "webpack-cli": "^4.2.0",
    "webpack-stream": "^6.1.1"
  }
}
